#include<bits/stdc++.h>
using namespace std;
void shiftzeros(int arr[],int n){
    int i=0;
    for(int j=0;j<n;j++){
        if(arr[j]!=0){
            swap(arr[i],arr[j]);
            i++;
        }
    }
    for(int i=0;i<n;i++){
        cout<<arr[i]<<endl;
    }
}
int main(){
    int arr[6]={0,12,0,0,3,1};
    shiftzeros(arr,6);
}
