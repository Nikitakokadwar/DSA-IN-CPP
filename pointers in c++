// pointer to pointer
#include<bits/stdc++.h>
using namespace std;
int main(){
   int num=5;
   int *ptr =& num;
   int **q = & ptr;
   cout<< *ptr<<endl;
   cout<<q;
   cout<<*ptr<<endl;
}
