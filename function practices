#include<bits/stdc++.h>
using namespace std;
void mainfunction (string name,int age){
    cout<<"name is "<<name<<"age is "<<age<<endl;
}
int main(){
    mainfunction("nikita",20);
    mainfunction("nitin",50);
    mainfunction("unmesh",25);
    mainfunction("nilesh",26);

}
