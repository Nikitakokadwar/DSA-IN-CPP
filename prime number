#include<bits/stdc++.h>
using namespace std;
bool isprime(int n){
    for(int i=2;i<n;i++){
        if(n%i==0)
        {
            return false;
        }
    }
     return true;
}
int main(){
    // cout<<"hello word"<<endl;
    int n;
    cin>>n;
    if(isprime(n)){
        cout<<"it is a prime number"<<endl;
    }
    else{
        cout<<":the no is not prime"<<endl;
    }

}
