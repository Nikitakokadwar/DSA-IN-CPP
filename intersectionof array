#include<bits/stdc++.h>
using namespace std;
void intersection(int arr1[],int arr2[],int m,int n){
for(int i=0;i<m;i++){
    for(int j=0;j<n;j++){
        if(arr1[i]==arr2[j]){
            cout<<arr1[i]<<" ";
        }
    }
}
}
int main(){
 int arr1[5]={1,2,4,5,6};
    int arr2[4]={2,3,5,7};
   

    intersection(arr1,arr2,5,4);
}
