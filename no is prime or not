#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    bool prime=true;
    for(int i=2;i<n/2;i++){
        if(n%i==0){
            cout<<"no is not prime"<<endl;
            prime=false;
        }
        else{
            cout<<"no is prime"<<endl;
            prime=true;
        }
    }
  
}
