#include<bits/stdc++.h>
using namespace std;
int main(){
    queue<int>q;
    q.push(10);
    cout<<"element present in the front of queue"<<" "<<q.front()<<endl;
     q.pop();
    q.push(11);
    cout<<"element present in the front of queue"<<" "<<q.front()<<endl;
    q.push(12);
    cout<<"size of the queue is"<<q.size()<<endl;
   
    q.pop();
    q.pop();
     cout<<"size of the queue is"<<q.size()<<endl;
    if(q.empty()){
        cout<<"the queue is empty"<<endl;
    }
    else{
        cout<<" the queue is not empty"<<endl;
    }
}
