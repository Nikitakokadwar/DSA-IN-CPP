#include<bits/stdc++.h>
using namespace std;
int main(){
    int flag=0;
    char string1[20]="nikin";
    int length;
    length=strlen(string1);
    for(int i=0;i<length; i++){
        if(string1[i]!= string1[length-i-1]);
        {
        flag=1;
        break;
    }
    }
    if(flag){
        cout<<string1<<"not a palindrome"<<endl;

    }
    else{
        cout<<string1<<"is a palindrome"<<endl;
    }
    
    
}
