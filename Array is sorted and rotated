#include<bits/stdc++.h>
using namespace std;
int sort(int arr[],int n){
    int counter=0;
    for(int i=0;i<n;i++){
        if(arr[i]>arr[i+1])
        counter++;
    }
   
    // for(int i=0;i<n;i++){
    //     // cout<<counter<<endl;;
    //     // cout<<arr[i]<<endl;
    // }
     if(counter>1)
    cout<<"false"<<endl;
    else
    cout<<"true"<<endl;
    //  cout<<counter;
    }
int main(){
   int arr[5]={3,4,5,1,2};
   sort(arr,5);
}
