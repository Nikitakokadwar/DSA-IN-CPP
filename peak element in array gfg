#include<bits/stdc++.h>
using namespace std;
int peak(int arr[],int n){
    int max=0;
    for(int i=0;i<n;i++){
        if(arr[i+1]>arr[i]){
            max=arr[i+1];
            continue;
        }
        else
        break;
        return max;
    }
}
int main(){
    int n;
    int arr[n]={10,30,70,60};
    int ans=peak(arr,n);
    cout<<ans;

}


